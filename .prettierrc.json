{
  "singleQuote": true,
  "printWidth": 120,
  "arrowParens": "always",
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": ["^react$", "^@(?!atlas|5minds).+",  "^@5minds(.+)", "^@atlas(.+)", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
