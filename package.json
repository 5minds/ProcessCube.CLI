{
  "name": "@atlas-engine/cli",
  "version": "2.0.0-alpha.8",
  "description": "Command Line Utilities for 5Minds ProcessCube",
  "main": "dist/pc.js",
  "scripts": {
    "clean": "rm -rf dist",
    "reinstall": "./reinstall.sh",
    "build": "esbuild src/pc.ts --bundle --outfile=dist/pc.js --platform=node --target=node10 --loader:.md=text",
    "lint": "eslint \"src/**/*.ts\"",
    "start": "node dist/pc.js",
    "test": "npm run test:unit",
    "test:unit": "mocha -r ts-node/register -r choma \"src/**/*.test.ts\"",
    "test:integration": "mocha --timeout 60000 --slow 10000 -r ts-node/register -r choma \"test/integration/*.test.ts\""
  },
  "bin": {
    "atlas": "./dist/pc.js",
    "processcube": "./dist/pc.js",
    "pc": "./dist/pc.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:atlas-engine/cli.git"
  },
  "author": "5Minds IT-Solutions GmbH & Co. KG",
  "license": "MIT",
  "devDependencies": {
    "@atlas-engine/atlas_engine_client": "^2.3.2",
    "@essential-projects/eslint-config": "^1.0.6",
    "@essential-projects/http": "^2.5.0",
    "@process-engine/ci_tools": "^3.2.1",
    "@process-engine/management_api_client": "^6.2.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.28",
    "@types/yargs": "^15.0.4",
    "bpmn-moddle": "^6.0.2",
    "chalk": "^2.4.2",
    "choma": "^1.2.1",
    "esbuild": "^0.11.5",
    "eslint": "^5.16.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "json5": "^2.1.1",
    "marked": "^0.8.0",
    "mime-types": "^2.1.18",
    "mocha": "^6.1.4",
    "node-fetch": "^2.6.0",
    "open": "^7.0.2",
    "os": "^0.1.1",
    "prettier": "^1.18.2",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.1.0",
    "tsconfig": "^7.0.0",
    "typescript": "^3.7.5",
    "yargs": "^15.4.1",
    "yesno": "^0.3.1"
  }
}
