{
  "name": "@5minds/processcube_cli",
  "version": "2.3.0",
  "description": "Command Line Utilities for 5Minds ProcessCube",
  "main": "dist/pc.js",
  "files": [],
  "scripts": {
    "clean": "rm -rf dist",
    "reinstall": "./reinstall.sh",
    "build-tsc": "tsc",
    "build": "esbuild src/pc.ts --bundle --outfile=dist/pc.js --platform=node --target=node20 --loader:.md=text --tsconfig=tsconfig.json",
    "prepare": "npm run build",
    "start": "node dist/pc.js",
    "test": "npm run test:unit",
    "test:unit": "mocha -r ts-node/register -r choma \"src/**/*.test.ts\"",
    "test:integration": "mocha --timeout 60000 --slow 10000 -r ts-node/register -r choma \"test/integration/*.test.ts\""
  },
  "bin": {
    "atlas": "./dist/pc.js",
    "processcube": "./dist/pc.js",
    "pc": "./dist/pc.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:atlas-engine/cli.git"
  },
  "author": "5Minds IT-Solutions GmbH & Co. KG",
  "license": "MIT",
  "dependencies": {
    "@5minds/processcube_engine_client": "^4.4.5",
    "@5minds/processcube_engine_sdk": "^4.0.3",
    "@npmcli/fs": "^3.1.0",
    "adm-zip": "^0.5.10",
    "bpmn-moddle": "^8.1.0",
    "chalk": "^4.0.0",
    "dayjs": "^1.11.10",
    "express": "^4.18.2",
    "glob": "^7.1.6",
    "json5": "^2.2.3",
    "lodash.mapvalues": "^4.6.0",
    "marked": "^11.2.0",
    "node-fetch": "^2.7.0",
    "open": "^10.0.3",
    "openid-client": "^5.6.4",
    "table": "^6.8.1",
    "tar": "^6.2.0",
    "yargs": "^17.7.2",
    "yesno": "^0.4.0"
  },
  "devDependencies": {
    "@types/lodash.mapvalues": "^4.6.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.16",
    "@types/table": "^6.3.2",
    "@types/yargs": "^17.0.32",
    "assert": "^2.1.0",
    "choma": "^1.2.1",
    "esbuild": "^0.20.0",
    "mocha": "^10.2.0",
    "prettier": "^3.2.4",
    "reflect-metadata": "^0.2.1",
    "ts-node": "^10.9.2",
    "tsconfig": "^7.0.0",
    "typescript": "^5.3.3"
  }
}
